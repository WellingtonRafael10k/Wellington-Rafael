function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(4);
}function setup() {
  createCanvas(600, 600);
  background("white");
}

function draw() {
  stroke("blue");
  fill("purple");

  // console.log(mouseIsPressed)

  if (mouseIsPressed) {
    rect(mouseX, mouseY, 20, 35);
  }
}

